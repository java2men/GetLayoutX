<?xml version="1.0" encoding="UTF-8"?>
<!--
Modification of the layout was made.
keyMap without modifier and keyMap modifier were merged.
For example, for iso = "SE00", where "S" means "Shift"
-->
<keyboard locale="en-t-k0-windows">
	<version platform="10" number="$Revision$"/>
	<names>
		<name value="US"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="`"/>
		<map iso="E01" to="1"/>
		<map iso="E02" to="2"/>
		<map iso="E03" to="3"/>
		<map iso="E04" to="4"/>
		<map iso="E05" to="5"/>
		<map iso="E06" to="6"/>
		<map iso="E07" to="7"/>
		<map iso="E08" to="8"/>
		<map iso="E09" to="9"/>
		<map iso="E10" to="0"/>
		<map iso="E11" to="-"/>
		<map iso="E12" to="="/>
		<map iso="D01" to="q"/>
		<map iso="D02" to="w"/>
		<map iso="D03" to="e"/>
		<map iso="D04" to="r"/>
		<map iso="D05" to="t"/>
		<map iso="D06" to="y"/>
		<map iso="D07" to="u"/>
		<map iso="D08" to="i"/>
		<map iso="D09" to="o"/>
		<map iso="D10" to="p"/>
		<map iso="D11" to="["/>
		<map iso="D12" to="]"/>
		<map iso="C01" to="a"/>
		<map iso="C02" to="s"/>
		<map iso="C03" to="d"/>
		<map iso="C04" to="f"/>
		<map iso="C05" to="g"/>
		<map iso="C06" to="h"/>
		<map iso="C07" to="j"/>
		<map iso="C08" to="k"/>
		<map iso="C09" to="l"/>
		<map iso="C10" to=";"/>
		<map iso="C11" to="&apos;"/>
		<map iso="C12" to="\"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="z"/>
		<map iso="B02" to="x"/>
		<map iso="B03" to="c"/>
		<map iso="B04" to="v"/>
		<map iso="B05" to="b"/>
		<map iso="B06" to="n"/>
		<map iso="B07" to="m"/>
		<map iso="B08" to=","/>
		<map iso="B09" to="."/>
		<map iso="B10" to="/"/>
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="SE00" to="~"/> <!-- ` -->
		<map iso="SE01" to="!"/> <!-- 1 -->
		<map iso="SE02" to="@"/> <!-- 2 -->
		<map iso="SE03" to="#"/> <!-- 3 -->
		<map iso="SE04" to="$"/> <!-- 4 -->
		<map iso="SE05" to="%"/> <!-- 5 -->
		<map iso="SE06" to="^"/> <!-- 6 -->
		<map iso="SE07" to="&amp;"/> <!-- 7 -->
		<map iso="SE08" to="*"/> <!-- 8 -->
		<map iso="SE09" to="("/> <!-- 9 -->
		<map iso="SE10" to=")"/> <!-- 0 -->
		<map iso="SE11" to="_"/> <!-- - -->
		<map iso="SE12" to="+"/> <!-- = -->
		<map iso="SD01" to="Q"/>
		<map iso="SD02" to="W"/>
		<map iso="SD03" to="E"/>
		<map iso="SD04" to="R"/>
		<map iso="SD05" to="T"/>
		<map iso="SD06" to="Y"/>
		<map iso="SD07" to="U"/>
		<map iso="SD08" to="I"/>
		<map iso="SD09" to="O"/>
		<map iso="SD10" to="P"/>
		<map iso="SD11" to="{"/> <!-- [ -->
		<map iso="SD12" to="}"/> <!-- ] -->
		<map iso="SC01" to="A"/>
		<map iso="SC02" to="S"/>
		<map iso="SC03" to="D"/>
		<map iso="SC04" to="F"/>
		<map iso="SC05" to="G"/>
		<map iso="SC06" to="H"/>
		<map iso="SC07" to="J"/>
		<map iso="SC08" to="K"/>
		<map iso="SC09" to="L"/>
		<map iso="SC10" to=":"/> <!-- ; -->
		<map iso="SC11" to="\u0022"/> <!-- '  to= "  -->
		<map iso="SC12" to="|"/> <!-- (key to right of ')  base=\ -->
		<map iso="SB00" to="|"/> <!-- (key to left of Z)  base=\ -->
		<map iso="SB01" to="Z"/>
		<map iso="SB02" to="X"/>
		<map iso="SB03" to="C"/>
		<map iso="SB04" to="V"/>
		<map iso="SB05" to="B"/>
		<map iso="SB06" to="N"/>
		<map iso="SB07" to="M"/>
		<map iso="SB08" to="&lt;"/> <!-- , -->
		<map iso="SB09" to="&gt;"/> <!-- . -->
		<map iso="SB10" to="?"/> <!-- / -->
		<map iso="SA03" to=" "/> <!-- space -->
	</keyMap>
</keyboard>
