		<map iso="C09" to="д"/> <!-- L -->
		<map iso="C10" to="ж"/> <!-- ; -->
		<map iso="C11" to="э"/> <!-- ' -->
		<map iso="C12" to="\"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="я"/> <!-- Z -->
