		<map iso="A03" to=" "/> <!-- space -->
		<map iso="SE00" to="Ё"/> <!-- ` -->
		<map iso="SE01" to="!"/> <!-- 1 -->
		<map iso="SE02" to="\u0022"/> <!-- 2  to= "  -->
